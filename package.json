    "next": "13.1.6",
    "playwright": "1.30.0",
    "playwright-aws-lambda": "0.9.0",
    "playwright-core": "1.30.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "use-count-up": "3.0.1",
    "zod": "3.20.2"
  },
  "devDependencies": {
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "autoprefixer": "10.4.13",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "postcss": "8.4.21",
    "prettier": "2.8.3",
