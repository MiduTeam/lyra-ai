{
  "name": "lyra-ai",
  "version": "0.1.0",
  "private": true,
  "author": "@aforina, @ikurotime, @nachoaldamav, @pheralb & @tmchein",
  "description": "An open-source app using Next.js 13 & Cohere API",
  "scripts": {
